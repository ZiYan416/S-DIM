ent/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('评价成功')\n          this.loadComments()\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    loadType() {\n      this.$request.get('/type/selectById?id=' + this.typeId).then(res => {\n        if (res.code === '200') {\n          this.typeData = res.data\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    addCollect() {\n      let data = {userId: this.user.id, typeId: this.typeId}\n      this.$request.post('/collect/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('收藏成功')\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    initDialog() {\n      this.fromVisible = true\n    },\n    save() {\n      let data = {\n        userId: this.user.id,\n        typeId: this.typeId,\n        hotelId: this.typeData.hotelId,\n        inTime: this.inTime,\n        outTime: this.outTime\n      }\n      this.$request.post('/orders/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('预订成功')\n          this.fromVisible = false\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>\n"],"mappings":"AAmNA;EAEAA,KAAA;IACA,IAAAC,MAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,QAAA;MACAT,MAAA,EAAAA,MAAA;MACAU,IAAA;MACAC,MAAA;MACAC,OAAA;MACAC,WAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;IACA,KAAAC,YAAA;EACA;EACA;EACAC,OAAA;IACAD,aAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,sCAAApB,MAAA,EAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,WAAA,GAAAQ,GAAA,CAAAvB,IAAA;QACA;UACA,KAAAyB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,aAAAC,QAAA,EAAAC,YAAA;MACA,KAAAA,YAAA;QACA,KAAAL,QAAA,CAAAM,OAAA;QACA;MACA;MACA,IAAA/B,IAAA;QACAgC,OAAA,EAAAF,YAAA;QACA7B,MAAA,OAAAA,MAAA;QACAgC,OAAA,OAA